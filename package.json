{
  "name": "@syncorix/ai-chat-sdk",
  "version": "0.1.1",
  "description": "Type-safe Socket.IO SDK for building real-time AI chat UIs in the browser (streaming tokens, typing indicators, presence, reconnects).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Syncorix-Global/AI-Chat.git"
  },
  "author": {
    "name": "Tosin Felixson-Yusuf",
    "email": "tosin@syncorixglobal.com",
    "url": "https://github.com/Syncorix-Global"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./typing-observer": {
      "types": "./dist/typing-observer.d.ts",
      "import": "./dist/typing-observer.mjs",
      "require": "./dist/typing-observer.cjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "typing-observer": [
        "dist/typing-observer.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@9.15.9",
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build && pnpm test",
    "mock:dev": "pnpm --filter mock-ai-chat-server dev",
    "playground:dev": "pnpm --filter ai-chat-playground dev",
    "dev:all": "concurrently -k \"pnpm mock:dev\" \"pnpm playground:dev\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint",
    "release": "chmod +x release && ./release"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "keywords": [
    "vite",
    "library",
    "typescript"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.36.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.12.0",
    "globals": "^16.4.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "react": "^19.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "socket.io-client": "^4.8.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs}": "eslint --fix"
  }
}
