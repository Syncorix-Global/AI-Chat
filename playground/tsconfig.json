{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": ["ES2020", "DOM"],
     "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@sockets/*": ["../src/sockets/*"],
      "@typingObserver/*": ["../src/interactions/typingObserver/*"]
    }
  },
  "include": ["src"]
}
